{
    "name":         "neutron/form-bundle",
    "description":  "This bundle provides extra form types empowered by jQuery",
    "keywords":     ["form", "jqueryui", "jQuery", "Symfony", "plupload", "tinymce", "inputlimiter", "select2", "recaptcha"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name":  "Nikolay Georgiev",
            "email": "azazen09@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "neutron/datagrid-bundle": ">=1.0.1,<2.0-dev",
        "imagine/imagine": ">=0.3.0"
    },
    "require-dev": {
        "neutron/component-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Neutron\\FormBundle": "" }
    },
    "target-dir": "Neutron/FormBundle"
}
